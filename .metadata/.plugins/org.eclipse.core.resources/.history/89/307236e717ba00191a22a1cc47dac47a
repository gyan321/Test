package com.practice.totalStringCharacters;

public class TotalCharectersFinding {

	static short count=0;
	public static void main(String[] args) {
		 String givenString=" I am from India ";
		 char [] ConvertedCharArray=givenString.toCharArray();
		 
		 for(char eachCharacter:ConvertedCharArray){
			 if(eachCharacter!=32){
				 count++;
			 }
		 }
		 
		 System.out.println("total characters :"+count) ;

	}

}
